        <condition/>
        <description>Checks if the opened date is not after the closed date</description>
        <field>opened_at</field>
        <global>true</global>
        <messages/>
        <name>NeedIt onChange opened/closed</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	var closeddate = g_form.getValue('closed_at');
	if (newValue > closeddate && newValue != '' && closeddate !='' ){
		g_form.setValue('opened_at', oldValue);
		alert ("The opened date can't be after the closed date");
		
	}
	else {
		return;
	}
}
]]></script>
